ory\";\n\nimport{BrowserRouter as Router, Route,Link,NavLink, Switch} from 'react-router-dom';\nimport './App.css';\nimport HomePage from './component/HomePage';\nimport { StockDetail } from './component/StockDetail';\n\n\nclass App extends Component {\n\n  render() {\n  \n  return (\n\n    <div className = \"App\">\n      <NavBar/>\n\n      <Switch>\n        \n        <Route path=\"/stocks/:id\" component = {StockDetail} />\n      \n        <Route path=\"/login\" component = {Login} />\n      \n        <Route path=\"/my-favorite\" component = {Favorite} />\n        \n        <Route path=\"/signup\" component = {Signup} />\n        \n        <Route path=\"/shop\" component = {Shop} />\n        \n\n\n        <Route path=\"/profile\" component = {Profile} />\n        \n        <Route path=\"/course\" component = {Course} />\n  \n        <Route path=\"/\" component = {HomePage} />\n      \n       </Switch>\n  \n    \n  </div>\n\n);\n}\n\n    \n}\n\nexport default App;","/Users/binodgurung/Documents/GitHub/Stockshare--frontend/stockshare--frontend/src/reportWebVitals.js",[],"/Users/binodgurung/Documents/GitHub/Stockshare--frontend/stockshare--frontend/src/component/Signup.js",["88"],"\nimport React, { Component } from \"react\";\nimport { Form, Button } from \"react-bootstrap\";\nconst headers = {\n  \"Content-Type\": \"application/json\",\n  Accept: \"application/json\",\n};\n\nexport default class SignUp extends Component {\n  state = {\n    name: \"\",\n    email: \"\",\n    password: \"\",\n  };\n\n  handleChange = (event) => {\n    this.setState({\n      [event.target.name]: event.target.value,\n    });\n  };\n\n  SignUp = (event) => {\n    event.preventDefault();\n    fetch(\"http://localhost:3000/users\", {\n      method: \"POST\",\n      headers: headers,\n      body: JSON.stringify({\n        name: this.state.name,\n        password: this.state.password,\n        email: this.state.email\n      }),\n    })\n      .then((resp) => resp.json())\n      .then((resp) => this.props.history.push(\"/login\"));\n  };\n\n  render() {\n    const { name, username, email, password, image,bio } = this.state;\n    return (\n      <div className = \"signup\">\n        <form onSubmit={this.SignUp}>\n          <div className=\"d-flex flex-column col-md-4 offset-md-4\">\n            <div className=\"form-group\">\n              <label for=\"inputdefault\">Name</label>\n              <input\n                className=\"form-control\"\n                id=\"inputdefault\"\n                name=\"name\"\n                type=\"text\"\n                placeholder=\"Name\"\n                value={name}\n                onChange={this.handleChange}\n              />\n            </div>\n\n          \n            <div className=\"form-group\">\n              <label for=\"inputdefault\">Email</label>\n              <input\n                className=\"form-control\"\n                id=\"inputdefault\"\n                name=\"email\"\n                type=\"text\"\n                placeholder=\"Email\"\n                value={email}\n                onChange={this.handleChange}\n              />\n            </div>\n\n            <div className=\"form-group\">\n              <label for=\"inputdefault\">Bio</label>\n              <input\n                className=\"form-control\"\n                id=\"inputdefault\"\n                name=\"bio\"\n                type=\"text\"\n                placeholder=\"Bio\"\n                value={bio}\n                onChange={this.handleChange}\n              />\n            </div>\n\n            <div className=\"form-group\">\n              <label for=\"inputdefault\">Image</label>\n              <input\n                className=\"form-control\"\n                id=\"inputdefault\"\n                name=\"image\"\n                type=\"text\"\n                placeholder=\"Enter URL\"\n                src={image}\n                onChange={this.handleChange}\n              />\n            </div>\n\n            <div className=\"form-group\">\n              <label for=\"inputdefault\">Username</label>\n              <input\n                className=\"form-control\"\n                id=\"inputdefault\"\n                name=\"username\"\n                type=\"text\"\n                placeholder=\"username\"\n                value={username}\n                onChange={this.handleChange}\n              />\n            </div>\n\n            <div className=\"form-group\">\n              <label for=\"inputdefault\">Password</label>\n              <input\n                className=\"form-control\"\n                id=\"inputdefault\"\n                name=\"password\"\n                type=\"password\"\n                placeholder=\"Password\"\n                value={password}\n                onChange={this.handleChange}\n              />\n            </div>\n            <Button variant=\"primary\" className=\"text-dark\" type=\"submit\">\n              Submit\n            </Button>\n          </div>\n        </form>\n        \n      </div>\n    );\n  }\n}\n\n\n\n","/Users/binodgurung/Documents/GitHub/Stockshare--frontend/stockshare--frontend/src/component/Shop.js",["89","90","91"],"import React, { Component } from 'react'\n\nexport class Shop extends Component {\n    render() {\n        return (\n           \n    <div className=\"row m-2\">\n        <div className=\"column\">\n            <img src=\"https://www.golfatoakland.com/wp-content/uploads/sites/7199/2018/12/gift-cards.png\" className=\"mb-2\"/><br></br>\n            <button type=\"submit\" className=\"btn\">Redeem points</button>\n        </div>\n        <div className=\"column\">\n            <img src=\"https://pisces.bbystatic.com/image2/BestBuy_US/Gallery/gl-21431-mybby-rewardpoints-23890.png;maxHeight=196;maxWidth=354\" className=\"mb-2\"/><br></br>\n            <button type=\"submit\" className=\"btn\">Buy points</button>\n        </div>\n        <div className=\"column\">\n            <img src=\"https://encryp